{
  "name": "@quantleaf/query-result",
  "version": "1.0.47",
  "description": "Query result definitions",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register 'test/**/*.ts'",
    "publish-npm": "npm --userconfig=./.npmrc-npm publish dist --access public",
    "publish-git": "npm --userconfig=./.npmrc-git publish dist",
    "publish-all": "npm run publish-npm && npm run publish-git",
    "build-publish": "npm run prepare-publish && npm run publish-all",
    "prepare-publish": "rimraf dist && tsc --project tsconfig.prod.json && ncp package.json dist/package.json",
    "patch-publish": "npm version patch && npm run build-publish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/quantleaf/query-result.git"
  },
  "author": "quantleaf",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^8.10.66",
    "chai": "^4.3.0",
    "mocha": "^8.2.1",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/quantleaf/query-result/issues"
  },
  "homepage": "https://github.com/quantleaf/query-result#readme",
  "directories": {
    "test": "test"
  }
}
